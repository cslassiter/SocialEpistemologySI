@article{ WOS:000323476700001,
Author = {Tobia, Kevin and Buckwalter, Wesley and Stich, Stephen},
Title = {Moral intuitions: Are philosophers experts?},
Journal = {PHILOSOPHICAL PSYCHOLOGY},
Year = {2013},
Volume = {26},
Number = {5},
Pages = {629-638},
Month = {OCT 1},
Abstract = {Psychologists and experimental philosophers have reported findings
   showing that in some cases ordinary people's moral intuitions are
   affected by factors of dubious relevance to the truth of the intuition.
   Some defend the use of intuition as evidence in ethics by arguing that
   philosophers are the experts in this area, and philosophers' moral
   intuitions are both different from those of ordinary people and more
   reliable. We conducted two experiments indicating that philosophers and
   non-philosophers do indeed sometimes have different moral intuitions,
   but challenging the notion that philosophers have better or more
   reliable intuitions.},
