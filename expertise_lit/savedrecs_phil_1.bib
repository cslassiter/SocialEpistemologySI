@article{ WOS:000429495800011,
Author = {Holman, Bennett and Berger, William J. and Singer, Daniel J. and Grim,
   Patrick and Bramson, Aaron},
Title = {Diversity and Democracy: Agent-Based Modeling in Political Philosophy},
