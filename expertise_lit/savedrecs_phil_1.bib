@article{ WOS:000075282400010,
Author = {Cunningham, MF and Kent, FH and Muir, D},
Title = {The Cyber-Olympics - Schools, sports and the superhighway},
Journal = {COMPUTERS \& EDUCATION},
Year = {1998},
Volume = {30},
Number = {1-2},
Pages = {61-65},
Month = {JAN-FEB},
Note = {CAL 97 Symposium on Superhighways, Super CAL, Super Learning, UNIV
   EXETER, EXETER, ENGLAND, MAR 23-26, 1997},
Organization = {Elsevier Sci; British Telecom; DfEE; Devon \& Cornwall TEC; Xemplar; ICL},
Abstract = {Increasing numbers of schools are connecting to the Internet and, as a
   consequence of this, there is a growing demand from schools for
   information on how to make effective use of this resource. However, it
   is an area in which very few teachers have expertise. Schools are
   actively seeking support and advice. The authors were interested in
   looking for uses of the Internet beyond browsing fdr information: how
   could the Internet be used to promote collaborative learning and
   stimulate work both on and off the computer? This paper describes a work
   in progress and presents a case study of a ``CyberOlympics{''} topic.
   The CyberOlympics project involved 17 primary schools from five
   different countries competing against each other in an international
   sporting event. It took place in 1996, the year of the Atlanta Olympic
   Games. While the Olympics were taking place many schools took the
   opportunity to run mini sporting events with their own pupils. The
   Internet allowed pupils to take part in these sporting events as an
   international competition without leaving their school grounds. This
   paper identifies some of the activities that benefited from Internet
   access, notes some of the difficulties involved in running such projects
   and makes suggestions as to how schools can best exploit this resource.
   (C) 1998 Elsevier Science Ltd. All rights reserved.},
