@article{ WOS:000340374600007,
Author = {Moore, Alfred},
Title = {DEMOCRATIC REASON, DEMOCRATIC FAITH, AND THE PROBLEM OF EXPERTISE},
Journal = {CRITICAL REVIEW},
Year = {2014},
Volume = {26},
Number = {1-2},
Pages = {101-114},
Abstract = {Helene Landemore's Democratic Reason develops one important line of
   research in political epistemology, which we can define as the study of
   the ways in which distributed knowledge is put together for the purposes
   of making political decisions. Landemore argues for the epistemic
   benefits of cognitive diversity in political decision procedures in a
   condition of epistemic equality-where there are no experts. Given this
   omission, her approach has undeveloped potential for a second line of
   research in political epistemology, on the problem of aggregating
   asymmetrically distributed knowledge, i.e., integrating democratic
   reason with expertise.},
