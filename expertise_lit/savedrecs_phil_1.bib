@article{ WOS:000400952400004,
Author = {Perez Otero, Manuel},
Title = {Theories of Reference, Experimental Philosophy, and the Calibration of
   Intuitions},
Journal = {THEORIA-REVISTA DE TEORIA HISTORIA Y FUNDAMENTOS DE LA CIENCIA},
Year = {2017},
Volume = {32},
Number = {1},
Pages = {41-62},
Month = {JAN},
Abstract = {E. Machery and some collaborators have used survey data to criticize
   Kripke's anti-descriptivism about proper names. I highlight a number of
   drawbacks in the tests of Machery et al. Some of my objections concern
   their ambiguity. In particular, the responses that according to them
   reveal descriptivist intuitions can be interpreted as anti-descriptivist
   responses, for reasons that as far as I know have not been pointed out
