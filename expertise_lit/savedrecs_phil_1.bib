@article{ WOS:000666140500001,
Author = {Ostrovskaya, Elena and Badmatsyrenov, Timur and Khandarov, Fyodor and
   Aktamov, Innokentii},
Title = {Russian-Speaking Digital Buddhism: Neither Cyber, nor Sangha},
