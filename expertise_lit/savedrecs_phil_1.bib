@article{ WOS:000316879100007,
Author = {Miller, Boaz},
Title = {When is consensus knowledge based? Distinguishing shared knowledge from
   mere agreement},
Journal = {SYNTHESE},
Year = {2013},
Volume = {190},
Number = {7},
Pages = {1293-1316},
Month = {MAY},
Abstract = {Scientific consensus is widely deferred to in public debates as a social
   indicator of the existence of knowledge. However, it is far from clear
   that such deference to consensus is always justified. The existence of
   agreement in a community of researchers is a contingent fact, and
   researchers may reach a consensus for all kinds of reasons, such as
   fighting a common foe or sharing a common bias. Scientific consensus, by
   itself, does not necessarily indicate the existence of shared knowledge
   among the members of the consensus community. I address the question of
   under what conditions it is likely that a consensus is in fact knowledge
   based. I argue that a consensus is likely to be knowledge based when
   knowledge is the best explanation of the consensus, and I identify three
   conditions-social calibration, apparent consilience of evidence, and
   social diversity, for knowledge being the best explanation of a
   consensus.},
Publisher = {SPRINGER},
