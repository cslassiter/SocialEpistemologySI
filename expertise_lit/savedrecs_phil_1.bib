@article{ WOS:000208659100005,
Author = {Ho, Anita},
Title = {TRUSTING EXPERTS AND EPISTEMIC HUMILITY IN DISABILITY},
Journal = {INTERNATIONAL JOURNAL OF FEMINIST APPROACHES TO BIOETHICS},
Year = {2011},
Volume = {4},
Number = {2},
Pages = {102-123},
Month = {FAL},
Abstract = {It is generally accepted that the therapeutic relationship between
   professionals and patients is one of trust. Nonetheless, some patient
   groups carry certain social vulnerabilities that can be exacerbated when
   they extend trust to health-care professionals. In exploring the
   epistemic and ethical implications of expert status, this paper examines
   how calls to trust may increase epistemic oppression and perpetuate the
   vulnerability of people with impairments. It critically evaluates the
