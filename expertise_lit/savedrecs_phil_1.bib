@article{ WOS:000288381500003,
Author = {Fuller, Steve},
Title = {THINKING THE UNTHINKABLE AS A RADICAL SCIENTIFIC PROJECT},
Journal = {CRITICAL REVIEW},
Year = {2010},
Volume = {22},
Number = {4},
Pages = {397-413},
Abstract = {Philip Tetlock underestimates the import of his own Expert Political
   Judgment. It is much more than a critical scientific evaluation of the
   accuracy and consistency of political pundits. It also offers a
   blueprint for challenging expertise more generally-in the name of
   scientific advancement. ``Thinking the unthinkable{''}-a strategy
   Tetlock employs when he gets experts to consider counterfactual
   scenarios that are far from their epistemic comfort zones-has had
   explosive consequences historically for both knowledge and morality by
   extending our sense of what is possible. Thinking the unthinkable might
   also put into question contemporary assumptions about the legitimacy of
   science.},
