@article{ WOS:000456391800002,
Author = {van Wietmarschen, Han},
Title = {Political testimony},
Journal = {POLITICS PHILOSOPHY \& ECONOMICS},
Year = {2019},
Volume = {18},
Number = {1},
Pages = {23-45},
Month = {FEB},
Abstract = {I argue that reliance on political testimony conflicts with two
   democratic values: the value of mutual justifiability and the value of
   equality of opportunity for political influence. Reliance on political
   testimony is characterized by a reliance on the assertions of others
   directly on a political question the citizen is asked to answer as part
   of a formal democratic decision procedure. Reliance on expert testimony
   generally, even in the context of political decision-making, does not
   similarly conflict with democratic values. As a consequence of the
   argument, citizens have a pro tanto reason to rely on their own
   political judgment when determining their vote, and democratic societies
   have a reason to only ask citizens questions they are able to answer
   without reliance on political testimony.},
