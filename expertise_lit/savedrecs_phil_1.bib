@article{ WOS:000461576700002,
Author = {Vanaman, Matthew and Leggett, Mary-Page and Crysel, Laura and Askew,
   Robert},
Title = {A Novel Measure of the Need for Moral Cognition},
Journal = {BASIC AND APPLIED SOCIAL PSYCHOLOGY},
Year = {2019},
Volume = {41},
Number = {1},
Pages = {20-33},
Abstract = {This research developed the first measure of the need for moral
   cognition, or the tendency to seek out, talk about, reflect on, or
   otherwise engage with ethical or moral issues. Interpretative
   phenomenological analysis was conducted on transcripts from interviews
   with members of the target population and focus groups with content
   experts. Pilot items were administered to a large online sample; 2
   latent factors were identified. Scores were assessed for test-retest
   reliability, and convergent-, discriminant-, and criterion-validity.
   Collectively, these studies demonstrate that the Need for Moral
   Cognition scale is a valid and reliable measure suitable for use in
   psychological research.},
