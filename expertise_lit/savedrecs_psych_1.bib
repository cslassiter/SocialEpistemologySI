@article{ WOS:A1992JP54300007,
Author = {GRANT, PR},
Title = {ETHNOCENTRISM BETWEEN GROUPS OF UNEQUAL POWER IN RESPONSE TO PERCEIVED
   THREAT TO SOCIAL IDENTITY AND VALUED RESOURCES},
Journal = {CANADIAN JOURNAL OF BEHAVIOURAL SCIENCE-REVUE CANADIENNE DES SCIENCES DU
   COMPORTEMENT},
