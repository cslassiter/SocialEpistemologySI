@article{ WOS:000323469600012,
Author = {Pachur, Thorsten and Marinello, Gianmarco},
Title = {Expert intuitions: How to model the decision strategies of airport
   customs officers?},
Journal = {ACTA PSYCHOLOGICA},
Year = {2013},
Volume = {144},
Number = {1},
Pages = {97-103},
Month = {SEP},
Abstract = {How does expertise impact the selection of decision strategies? We asked
   airport customs officers and a novice control group to decide which
   passengers (described on several cue dimensions) they would submit to a
   search. Additionally, participants estimated the validities of the
   different cues. Then we modeled the decisions using compensatory
   strategies, which integrate many cues, and a noncompensatory heuristic,
   which relies on one-reason decision making. The majority of the customs
   officers were best described by the noncompensatory heuristic, whereas
   the majority of the novices were best described by a compensatory
   strategy. We also found that the experts' subjective cue validity
   estimates showed a higher dispersion across the cues and that
   differences in cue dispersion partially mediated differences in strategy
   use between experts and novices. Our results suggest that experts often
   rely on one-reason decision making and that expert-novice differences in
   strategy selection may reflect a response to the internal representation
   of the environment. (C) 2013 Elsevier B.V. All rights reserved.},
