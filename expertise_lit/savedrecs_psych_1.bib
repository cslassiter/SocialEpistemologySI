@article{ WOS:000083955600010,
Author = {Windschitl, PD and Weber, EU},
Title = {The interpretation of ``likely{''} depends on the context, but
   ``70\%{''} is 70\% - Right? The influence of associative processes on
   perceived certainty},
Journal = {JOURNAL OF EXPERIMENTAL PSYCHOLOGY-LEARNING MEMORY AND COGNITION},
Year = {1999},
Volume = {25},
Number = {6},
Pages = {1514-1533},
Month = {NOV},
Abstract = {Past research has demonstrated that interpretations of vague verbal
   forecasts (e.g., ``likely{''}) differ as a function of the context to
