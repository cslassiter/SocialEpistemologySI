@article{ WOS:000295234300006,
Author = {de Boer, Benjamin J. and van Hooft, Edwin A. J. and Bakker, Arnold B.},
Title = {Stop and Start Control: A Distinction within Self-control},
Journal = {EUROPEAN JOURNAL OF PERSONALITY},
Year = {2011},
Volume = {25},
Number = {5},
Pages = {349-362},
Month = {SEP-OCT},
Abstract = {A theoretical distinction within self-control, between stop control and
   start control, was investigated in two studies. Study 1 consisted of a
   pilot study in which expert ratings of existing self-control items were
   used to distinguish between stop and start control items and a
   confirmatory factor analyses of these items using a student sample (N =
   474). Also, stop and start control were related to overall affect and
   behavioural outcomes. Stop control was negatively related to negative
   affect, whereas start control was positively related to positive affect.
   Study 2 (N = 226) replicated some of these findings; stop control was
   the best predictor (-) of smoking and alcohol consumption whereas start
   control was the best predictor (+) of exercising and studying. Copyright
   (C) 2010 John Wiley \& Sons, Ltd.},
