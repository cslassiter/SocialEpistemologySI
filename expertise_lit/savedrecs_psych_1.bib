@article{ WOS:000167377800004,
Author = {Brailey, K and Vasterling, JJ and Franks, JJ},
Title = {Memory of psychodiagnostic information: Biases and effects of expertise},
Journal = {AMERICAN JOURNAL OF PSYCHOLOGY},
Year = {2001},
Volume = {114},
Number = {1},
Pages = {55-92},
Month = {SPR},
Abstract = {Problem-solving expertise has been associated with enhanced memory of
   domain-specific information. This enhanced memory is thought to play an
   important role in expert decisions. Meanwhile. research on
   psychodiagnostic decision making has found consistent limitations in
   experienced clinicians' ability to make optimal decisions. To what
   extent are these limitations associated with suboptimal memory
   processes? We compared memories of expert clinicians and novice graduate
   students for information learned while viewing a videotaped
   psychodiagnostic interview. Results of 3 tests suggest that expert
   clinicians exhibit enhanced memory that is flexible, selective, and
   accurate but with limitations that might contribute to poor decisions.
   Experts exhibited superior memory of personal criteria and
   disconfirmatory information. However, a framing manipulation induced
   performance in experts consistent with suboptimal decision making, and
   both groups needed exhaustive prompts for optimal memory search.
   Implications of these findings for expertise models are discussed.},
Publisher = {UNIV ILLINOIS PRESS},
Address = {1325 S OAK ST, CHAMPAIGN, IL 61820 USA},
Type = {Article},
Language = {English},
Affiliation = {Brailey, K (Corresponding Author), New Orleans Vet Affairs Med Ctr, New Orleans, LA USA.
   New Orleans Vet Affairs Med Ctr, New Orleans, LA USA.
   Tulane Univ, Sch Med, Dept Psychiat \& Neurol, New Orleans, LA 70118 USA.
