Title = {Historically rice-farming societies have tighter social norms in China
   and worldwide},
Journal = {PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA},
Year = {2020},
Volume = {117},
