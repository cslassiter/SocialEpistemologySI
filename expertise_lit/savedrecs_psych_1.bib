@article{ WOS:000171355900005,
Author = {Nickerson, RS},
Title = {The projective way of knowing: A useful heuristic that sometimes
   misleads},
Journal = {CURRENT DIRECTIONS IN PSYCHOLOGICAL SCIENCE},
