@article{ WOS:000283208500008,
Author = {del Prado, Alicia M. and Church, A. Timothy},
Title = {Development and Validation of the Enculturation Scale for Filipino
   Americans},
Journal = {JOURNAL OF COUNSELING PSYCHOLOGY},
