Author = {Cramer, Robert J. and Kehn, Andre and Pennington, Charlotte R. and
   Wechsler, Hayley J. and Clark, III, John W. and Nagle, Jacklyn},
Title = {An Examination of Sexual Orientation- and Transgender-Based Hate Crimes
   in the Post-Matthew Shepard Era},
