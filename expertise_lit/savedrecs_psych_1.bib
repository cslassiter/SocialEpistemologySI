@article{ WOS:000081598700003,
Author = {Bar-Tal, Y and Raviv, A and Spitzer, A},
Title = {The need and ability to achieve cognitive structuring: Individual
   differences that moderate the effect of stress on information processing},
Journal = {JOURNAL OF PERSONALITY AND SOCIAL PSYCHOLOGY},
Year = {1999},
Volume = {77},
Number = {1},
Pages = {33-51},
Month = {JUL},
Note = {15th Bi-Annual Conference on Subjective Probability, Utility and
   Decision Making (SPUDM15), JERUSALEM, ISRAEL, AUG 20-24, 1995},
Organization = {European Assoc Decis Making},
Abstract = {The authors examined the hypothesis that the interaction between the
   need for cognitive structure (NCS) and the ability to achieve cognitive
   structure (AACS) moderates the effect of stress on information
   processing. NCS is the preference for using cognitive structuring, as
   opposed to piecemeal processes, as a means to achieve certainty. AACS is
   the extent to which individuals are able to apply information processes
   that are consistent with their levels of NCS. The hypothesis was
   validated in 4 studies, which showed that stress increased high-AACS
   participants' use of cognitive structuring if they had high NCS and
   reduced it if they had low NCS. An opposite effect was found for
   low-AACS participants. The implications of these results for the
   understanding of the mechanisms responsible for the effect of stress on
   information processing are discussed.},
