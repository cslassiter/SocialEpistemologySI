@article{ WOS:A1992HP11500025,
Author = {FISCHHOFF, B},
Title = {GIVING ADVICE - DECISION-THEORY PERSPECTIVES ON SEXUAL ASSAULT},
Journal = {AMERICAN PSYCHOLOGIST},
Year = {1992},
