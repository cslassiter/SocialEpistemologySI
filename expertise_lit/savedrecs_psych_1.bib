@article{ WOS:A1995RE71200004,
Author = {HINSZ, VB and INDAHL, KE},
Title = {ASSIMILATION TO ANCHORS FOR DAMAGE AWARDS IN A MOCK CIVIL TRIAL},
Journal = {JOURNAL OF APPLIED SOCIAL PSYCHOLOGY},
Year = {1995},
Volume = {25},
Number = {11},
Pages = {991-1026},
Month = {JUN 1},
Note = {1993 Annual Meeting of the Judgment-and-Decision-Making-Society,
   WASHINGTON, DC, 1993},
Organization = {Judgment \& Decis Making Soc},
Abstract = {Some judicial reforms propose that limits on damage awards in civil
   trials will lower the amounts awarded while still maintaining
   perceptions of justice.  This article describes research that examines
   the impact of damage award limits on mock juror judgments.  Existing
   theory suggests that limits will serve as anchors for the mock jurors'
   judgments.  Predictions that damage awards would move toward the anchor
   introduced by a limit were supported.  This research suggests that
   depending on the location of the anchor on the response scale, limits
   may not have the intended effects.  Instead, limits may actually
   increase damage awards, and may also reduce juror's perceptions of
   fairness of the damage award judgments.},
