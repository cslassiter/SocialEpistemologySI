@article{ WOS:000233018000009,
Author = {Schuller, RA and Terry, D and McKimmie, B},
Title = {The impact of expert testimony on jurors' decisions: Gender of the
   expert and testimony complexity},
Journal = {JOURNAL OF APPLIED SOCIAL PSYCHOLOGY},
Year = {2005},
Volume = {35},
Number = {6},
Pages = {1266-1280},
Month = {JUN},
Abstract = {The present study investigated whether people used the gender of an
   expert witness as a heuristic cue to evaluate the evidence presented by
   the expert. Specifically, the gender of the expert and the complexity of
   the expert's testimony (low, high) were varied systematically within a
   simulated civil trial involving an antitrust price-fixing agreement. It
   was expected that the male expert would be more persuasive than the
   female expert, but only when the testimony presented was complex. As
   predicted, this interaction was revealed across a range of dependent
   measures. Somewhat unexpected was the finding of a female expert
   advantage in the low-complexity condition. The implications of these
   findings are discussed.},
