@article{ WOS:A1992JK36800010,
Author = {TETLOCK, PE},
Title = {GOOD JUDGMENT IN INTERNATIONAL-POLITICS - 3 PSYCHOLOGICAL PERSPECTIVES},
Journal = {POLITICAL PSYCHOLOGY},
