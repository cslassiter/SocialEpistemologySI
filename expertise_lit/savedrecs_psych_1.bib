@article{ WOS:000430241000004,
Author = {Kardas, Michael and O'Brien, Ed},
Title = {Easier Seen Than Done: Merely Watching Others Perform Can Foster an
   Illusion of Skill Acquisition},
Journal = {PSYCHOLOGICAL SCIENCE},
Year = {2018},
Volume = {29},
Number = {4},
Pages = {521-536},
Month = {APR},
Abstract = {Modern technologies such as YouTube afford unprecedented access to the
   skilled performances of other people. Six experiments (N = 2,225) reveal
   that repeatedly watching others can foster an illusion of skill
   acquisition. The more people merely watch others perform (without
   actually practicing themselves), the more they nonetheless believe they
   could perform the skill, too (Experiment 1). However, people's actual
   abilities-from throwing darts and doing the moonwalk to playing an
   online game-do not improve after merely watching others, despite
   predictions to the contrary (Experiments 2-4). What do viewers see that
   makes them think they are learning? We found that extensive viewing
   allows people to track what steps to take (Experiment 5) but not how
   those steps feel when taking them. Accordingly, experiencing a taste of
   performing attenuates the illusion: Watching others juggle but then
   holding the pins oneself tempers perceived change in one's own ability
   (Experiment 6). These findings highlight unforeseen problems for
   self-assessment when watching other people.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Kardas, M (Corresponding Author), Univ Chicago, Booth Sch Business, 5807 South Woodlawn Ave, Chicago, IL 60637 USA.
   Kardas, Michael; O'Brien, Ed, Univ Chicago, Booth Sch Business, 5807 South Woodlawn Ave, Chicago, IL 60637 USA.},
DOI = {10.1177/0956797617740646},
ISSN = {0956-7976},
EISSN = {1467-9280},
Keywords = {self-assessment; empathy gap; repeated exposure; open data; open
