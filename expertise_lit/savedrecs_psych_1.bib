@article{ WOS:000079392300003,
Author = {Phillips, JM},
Title = {Antecedents of leader utilization of staff input in decision-making
   teams},
Journal = {ORGANIZATIONAL BEHAVIOR AND HUMAN DECISION PROCESSES},
