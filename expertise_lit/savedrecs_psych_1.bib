@article{ WOS:000270189300009,
Author = {Broomell, Stephen B. and Budescu, David V.},
Title = {Why Are Experts Correlated? Decomposing Correlations Between Judges},
Journal = {PSYCHOMETRIKA},
Year = {2009},
Volume = {74},
Number = {3},
Pages = {531-553},
Month = {SEP},
Abstract = {We derive an analytic model of the inter-judge correlation as a function
   of five underlying parameters. Inter-cue correlation and the number of
   cues capture our assumptions about the environment, while
   differentiations between cues, the weights attached to the cues, and
   (un)reliability describe assumptions about the judges. We study the
   relative importance of, and interrelations between these five factors
   with respect to inter-judge correlation. Results highlight the
   centrality of the inter-cue correlation. We test the model's predictions
   with empirical data and illustrate its relevance. For example, we show
   that, typically, additional judges increase efficacy at a greater rate
   than additional cues.},
