@article{ WOS:000074672500005,
Author = {Highhouse, S},
Title = {Understanding and improving job-finalist choice: The relevance of
   behavioral decision research},
Journal = {HUMAN RESOURCE MANAGEMENT REVIEW},
Year = {1997},
Volume = {7},
Number = {4},
Pages = {449-470},
Month = {WIN},
Abstract = {This article highlights the relevance of behavioral decision research to
   the task of choosing one job candidate from a small set of comparable
   finalists (e.g., executive placement, presidential election). A
   conceptual framework is presented which views job-finalist choice as a
   quasi-rational process, composed of both analytical and intuitive
   sub-processes. Research needs for improving job-finalist choice are
   presented, as well as a consideration of issues and controversies
   involved in the generalization of behavioral decision research to
   employment settings. Finally, the appropriateness of focusing on
   anomalies in job-finalist choice is discussed.},
