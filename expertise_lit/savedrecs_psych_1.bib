@article{ WOS:000267345300011,
Author = {Christandl, Fabian and Fetchenhauer, Detlef},
Title = {How laypeople and experts misperceive the effect of economic growth},
Journal = {JOURNAL OF ECONOMIC PSYCHOLOGY},
Year = {2009},
Volume = {30},
Number = {3},
Pages = {381-392},
Month = {JUN},
Abstract = {A series of four experiments were performed to examine the accuracy of
   estimations of economic growth by both experts and lay people, the
   factors that influence the accuracy of their estimations, and which
   procedures they use to make estimations. The results show that for
   actual growth rates higher than 1\%, both groups clearly underestimated
   growth, and the underestimation was lower for experts than for
   laypeople. The estimations became slightly better when the task was
   presented in a financial investment scenario. Incentives had no effect
   on the accuracy of the estimations: however, a positive influence of the
   need for cognition was observed. Male participants provided more
   accurate estimations than female participants. The common use of
   different, and inappropriate solution procedures, accounted for the
   under-estimators. (C) 2009 Elsevier B.V. All rights reserved.},
