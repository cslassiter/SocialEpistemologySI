@article{ WOS:000242632400006,
Author = {Fu, Jeanne Ho-Ying and Chiu, Chi-Yue and Morris, Michael W. and Young,
   Maia J.},
Title = {Spontaneous inferences from cultural cues - Varying responses of
   cultural insiders and outsiders},
Journal = {JOURNAL OF CROSS-CULTURAL PSYCHOLOGY},
Year = {2007},
Volume = {38},
Number = {1},
Pages = {58-75},
Month = {JAN},
Abstract = {Results from two groups of biculturals (Hong Kong undergraduates,
   Chinese Americans) and a group of European Americans in two studies
   showed that in the presence of applicable cues of a culture, individuals
   with expert knowledge in the culture spontaneously make inferences about
   the culture's moral values, producing a Stroop-like effect. Although
   both biculturals and European Americans made spontaneous cultural
   inferences from American cultural cues, only biculturals made
   spontaneous inferences from Chinese cultural cues. Moreover,
   American-Chinese bicultural individuals can switch between correspondent
   cultural inferences from American and Chinese cultural cues numerous
   times within one experimental session. Implications on cultural
   adaptation and cultural competence are discussed.},
