@article{ WOS:000297292600018,
Author = {Mendel, R. and Traut-Mattausch, E. and Jonas, E. and Leucht, S. and
   Kane, J. M. and Maino, K. and Kissling, W. and Hamann, J.},
Title = {Confirmation bias: why psychiatrists stick to wrong preliminary
   diagnoses},
