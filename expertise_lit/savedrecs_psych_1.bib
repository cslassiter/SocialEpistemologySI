@article{ WOS:000075330100007,
Author = {Jackson, JP},
Title = {Creating a consensus: Psychologists, the Supreme Court, and school
   desegregation, 1952-1955},
Journal = {JOURNAL OF SOCIAL ISSUES},
Year = {1998},
Volume = {54},
Number = {1},
Pages = {143-177},
Month = {SPR},
Abstract = {SPSSI psychologists' involvement in the early civil rights movement in
   the postwar United States was epitomized by their involvement in the
   case of Brown v. Board of Education. This article examines how social
   scientists sought to maintain the persona of objective, scientific
   expert when asked to prepare briefs for the US. Supreme Court for the
   Brown case. The social scientists believed that only by collapsing what
   they saw as an artificial distinction between objectivity and advocacy
   could the social scientist become a social activist This article is
   based on extensive research in numerous archives, including the papers
   of Gordon W. Allport, Kenneth B. Clark, Stuart W. Cook, David Krech, the
   National Association for the Advancement of Colored People, Theodore M.
