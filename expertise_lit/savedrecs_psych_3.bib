@article{ WOS:A1993LB53800007,
Author = {VANIJZENDOORN, WJE and BUS, AG},
Title = {HOW VALID ARE EXPERTS PROGNOSES ON CHILDREN WITH LEARNING-PROBLEMS},
Journal = {JOURNAL OF SCHOOL PSYCHOLOGY},
Year = {1993},
Volume = {31},
Number = {2},
Pages = {317-325},
Month = {SUM},
Abstract = {All assessments of children contain more or less explicit predictions
   about the pupils' performance. In the present study the validity of
   prognoses for children with learning problems was tested. On the basis
   of psychological and educational tests ten education students, ten
   practicing school psychologists, and ten directors of special schools
   diagnosed learning problems in four children. In addition they gave
   their prognoses for the pupils' development over the following 6 months
   in the cognitive and the social-emotional domain. The validity of the
   prognoses was tested by comparing the experts' statements with the
   pupils' actual development. On the average the prognoses appeared very
   inaccurate. The prognoses in the cognitive domain were somewhat more
   valid than the prognoses in the social-emotional domain. There were no
   differences among the three groups. However, the accuracy of the
   prognoses varied with the problem at hand.},
