@article{ WOS:000499659000009,
Author = {Knausenberger, Judith and Wagner, Ullrich and Higgins, E. Tory and
   Echterhoff, Gerald},
Title = {Epistemic Authority in Communication Effects on Memory: Creating Shared
   Reality with Experts on the Topic},
Journal = {JOURNAL OF APPLIED RESEARCH IN MEMORY AND COGNITION},
