@article{ WOS:000648834200004,
Author = {Bailey, Phoebe E. and Ebner, Natalie C. and Moustafa, Ahmed A. and
   Phillips, Joseph R. and Leon, Tarren and Weidemann, Gabrielle},
Title = {The Weight of Advice in Older Age},
Journal = {DECISION-WASHINGTON},
Year = {2021},
Volume = {8},
Number = {2},
Pages = {123-132},
Month = {APR},
Abstract = {Seeking advice from others may improve decision-making, particularly in
   older adults when cognitive decline can impair decision-making. This
   study measured the extent to which older adults rated the value of
   advice and used that advice in their decisions. Young (aged 18-37 years;
   n = 57) and older (aged 62- 84 years; n = 56) adults completed a
   judge-advisor task incorporating advice from an expert and a novice. To
   capture interindividual differences in ratings of advice value and
   advice use (i.e., weight of advice), desire for autonomy, working
   memory, and fluid intelligence were assessed. Relative to young adults,
   older adults rated novice advice as being more valuable and were more
   likely to adjust their estimates based on expert and especially novice
   advice. Among older adults, poorer working memory and reduced preference
   for autonomous decision-making were associated with greater ratings of
   the value of novice advice, while better fluid intelligence was
   associated with increased ratings of the value of expert advice.
   Overall, older adults give more weight to advice and cognitive decline
   appears to compromise discrimination of the quality of that advice.},
