@article{ WOS:000187991400004,
Author = {Reimer, T},
Title = {Direct and indirect effects of argument quality: The impact of argument
   strength on the perceived expertise of a communicator},
Journal = {ZEITSCHRIFT FUR SOZIALPSYCHOLOGIE},
Year = {2003},
Volume = {34},
Number = {4},
Pages = {243-255},
Month = {DEC},
Abstract = {In persuasion research (e.g., Petty, Cacioppo, \& Goldman, 1981), source
   expertise and argument quality are often manipulated as experimental
   factors. However, the attempt to manipulate these two experimental
   factors independently usually fails and thus the internal validity of
   the experiments is reduced. This study aimed at testing for an indirect
   effect of argument strength on attitudes, an effect that is mediated by
   perceived source expertise. One-hundred undergraduate psychology
   students were asked to read a fictitious newspaper article on the new
   set-up of the credit point system in psychology at the University of
   Basel. As independent variables, source expertise (high/low/no
   information) and argument quality (high/low) were varied. Results
   confirmed the assumption that argument quality has not only an effect on
   the attitude toward the change in the credit point system, but has also
   a strong effect on the perceived expertise of the author. Moreover,
   mediational analyses supported the claim that the effect of argument
   strength on the attitude was partially mediated by its effect on
   perceived source expertise.},
