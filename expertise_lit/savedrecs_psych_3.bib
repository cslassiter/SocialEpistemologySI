@article{ WOS:000263347200036,
Author = {Gomlekslz, Mufit and Poyrazli, Senel and Vural, Ruken Akar},
Title = {DISCRIMINATORY ATTITUDES: A SCALE DEVELOPMENT IN TURKISH},
