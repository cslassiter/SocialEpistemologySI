@article{ WOS:000739426200001,
Author = {Pozzi, Maura and Passini, Stefano and Chayinska, Maria and Morselli,
   Davide and Ellena, Adriano Mauro and Wlodarczyk, Anna and Pistoni, Carlo},
Title = {`Coming together to awaken our democracy': Examining precursors of
   emergent social identity and collective action among activists and
   non-activists in the 2019-2020 `Chile desperto' protests},
Journal = {JOURNAL OF COMMUNITY \& APPLIED SOCIAL PSYCHOLOGY},
Year = {2022},
Volume = {32},
