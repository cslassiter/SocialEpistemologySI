@article{ WOS:000261042400009,
Author = {Wright, Keith},
Title = {MEASURES OF UNCERTAINTY IN RULE-BASED SYSTEMS: ONE IS NOT ENOUGH},
Journal = {JOURNAL OF COMPUTER INFORMATION SYSTEMS},
Year = {2008},
Volume = {49},
Number = {1},
Pages = {67-72},
Month = {FAL},
Abstract = {The solution of most commercial rule-based expert systems consists of
   two components - the conclusion reached and its calculated measure of
   belief, expressed as a single number. In non-deterministic domains, this
   number is often the most critical factor in analyzing the solution.
   However, as this paper reviews, a robust calculus for this number has
   yet to be documented. Thus reliance on it alone poses a risk to users of
   commercial systems. This paper demonstrates how supplemental statistics
   can be calculated and used to decrease this risk. We hope these ideas
   will lead to better commercial expert systems and better user decision.
   making.},
