@article{ WOS:A1995RC52700001,
Author = {REARDON, R and DOYLE, SM},
Title = {THE SELF-CONCEPT AND REALITY JUDGMENTS - MEMORY, MEMORY MONITORING, AND
   INTERNAL-EXTERNAL CORRESPONDENCE},
Journal = {SOCIAL COGNITION},
Year = {1995},
Volume = {13},
Number = {1},
Pages = {1-24},
Month = {SPR},
Abstract = {The role of the self-concept in helping us make reality judgments was
   investigated from the perspective of M. Johnson's (1983, 1991)
   Multiple-Entry Modular Memory System and the internal/external
   correspondence (Brickman, 1978) individual difference variable. Subjects
   classified as internally correspondent (IC) or externally correspondent
   (EC) on the Internal Correspondence Scale (Ickes and Teng, 1987) were
   run in a modified reality monitoring paradigm (Johnson and Raye, 1981);
   that is, they read and generated pleasant and unpleasant personality
   trait words that were embedded in first or third person sentence frames,
   and they reported the degree to which they believed the traits were true
   of themselves. The results indicated that ICs' recognition memory for
   the trait words was affected by pleasantness and trueness only when the
   words were ones that they generated. ECs' recognition memory was
   similarly affected for trait words that they had read. Analyses on
   reality monitoring ability revealed that when the trait words were
   encoded from first person sentence frames, there were no significant
   differences between the ICs' and ECs' ability to monitor the source
   (read/generate) of the words. When the trait words were encoded from
   third person sentence frames, ICs and ECs were found to have different
   patterns of reality monitoring. These results support the view that
   basic memorial processes may be characterized by individual differences
   in one's usage of memory subsystems defined by the internal/external
   correspondence construct, and that this construct is associated with
   different uses of the self-construct in evaluating reality.},
