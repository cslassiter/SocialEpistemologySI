@article{ WOS:000439676900003,
Author = {Spassova, Gerri and Palmeira, Mauricio and Andrade, Eduardo B.},
Title = {A ratings pattern heuristic in judgments of expertise: When being right
   Looks wrong},
Journal = {ORGANIZATIONAL BEHAVIOR AND HUMAN DECISION PROCESSES},
Year = {2018},
Volume = {147},
Pages = {26-47},
Month = {JUL},
Abstract = {We propose a ``ratings pattern heuristic{''} in judgments of expertise
   that is, people's tendency to undervalue critics who assign the same
   rating to multiple options, overlooking diagnostic information which
   would clearly justify the uniform ratings. The heuristic is driven by a
   strong association between discrimination and expertise and a focus on
   summary ratings. People ``punish{''} uniform (vs. varied) raters even
   when (a) uniform ratings are acknowledgedly more likely (studies la and
   ib), (b) the uniform rater's past performance is superior (studies 2 and
   3), and (c) the uniform rater also reports varied sub-ratings (study
   4a), unless participants are prompted to assess the sub-ratings prior to
   choosing a critic (studies 4b and 5). Study 6 reveals that critics are
   less aware than judges of the impact of the pattern of their ratings on
   others' perceptions.},
