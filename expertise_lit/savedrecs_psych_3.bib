@article{ WOS:000721735800007,
Author = {Graf, Ruediger},
Title = {OF ALCOHOL, APES, AND TAXES: GuNTER SCHMoLDERS AND THE REINVENTION OF
   ECONOMICS IN BEHAVIORAL TERMS},
Journal = {JOURNAL OF THE HISTORY OF ECONOMIC THOUGHT},
Year = {2021},
Volume = {43},
