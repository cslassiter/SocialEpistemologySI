@article{ WOS:A1997XL75300021,
Author = {Furnham, A},
Title = {The positive consequences of being negative: The perception of a
   critical reviewer},
Journal = {JOURNAL OF SOCIAL BEHAVIOR AND PERSONALITY},
Year = {1997},
Volume = {12},
Number = {2},
Pages = {585-594},
Month = {JUN},
Abstract = {This study was a partial replication and extension of the work of
   Amabile (1983). She tested the assumption that only pessimism sounds
   profound; optimism sounds superficial. Sire demonstrated that negative
   valuations of intellectual products (book reviews) were perceived as
   more intelligent than positive evaluations. In this study, 100
   participants were asked to read and rate four book reviews which were
   counter-balanced as either positive or negative. They were then required
   to rate the reviewer, the book and the author on eight bi-polar scales.
   There was a highly significant effect on all ratings of the reviewer,
   the book and the author when comparing positive versus negative reviews.
   Compared to reviewers who gave positive evaluations, negative reviewers
   were seen as having less literary expertise, less competence and less
   intelligence. This contradicts Amabile's study. Possible explanations
   for the significant differences between the two similar studies are
   explored.},
