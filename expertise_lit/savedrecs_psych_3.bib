@article{ WOS:000172059900006,
Author = {Jonas, E and Frey, D and Henninger, M and Pommer, M and Schulz-Hardt, S
   and von Haeften, I and Mandl, H},
Title = {Explaining one's own behavior as a factor influencing the search for and
   evaluation of information in a sequential learning situation},
Journal = {ZEITSCHRIFT FUR ENTWICKLUNGSPSYCHOLOGIE UND PADAGOGISCHE PSYCHOLOGIE},
Year = {2001},
Volume = {33},
Number = {4},
Pages = {242-252},
Abstract = {This article combines the perspectives of social and educational
   psychology in examining the question of how the explanation of choices
   made within a learning situation influences the search for information
   and its evaluation. 72 participants took part in a communication
   experiment where they bad to choose one of five possible statements in
   three consecutive sequences. Two variables were manipulated: verbal
   explanation of the selected statement and confrontation with information
   in the form of expert statements. Both constitute important aspects of a
   learning environment related to situated learning approaches within
   educational psychology. From a social psychological perspective,
   verbalizing and explaining one's choice increases the commitment to the
   chosen alternative. This leads, to a biased search for additional
   information: that is, information supporting the choice made is
   preferred over information conflicting with the choice. This selective
   search for confirmatory information, however, may involve negative
   effects on learning. The results of the reported experiment show that
   participants who were obliged to explain their choices showed a stronger
   preference for supportive information than did participants who did not
   have to verbalize their choices. In addition, the participants who
   explained their choices displayed a poorer performance - in terms of
   fewer correct answers - than did those who did not have to verbalize
   their choices. Further results showed that the search for supportive
   information decreased from sequence to sequence; in other words, the.
   participants became more open to conflicting information. Implications
   of these social psychological findings for improving educational
   psychological approaches are discussed.},
