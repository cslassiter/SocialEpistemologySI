@article{ WOS:A1994PP48300004,
Author = {KLEINMUNTZ, B},
Title = {TOWARD INTELLIGENT COMPUTERIZED CLINICIAN},
Journal = {COMPUTERS IN HUMAN BEHAVIOR},
Year = {1994},
Volume = {10},
Number = {4},
Pages = {457-466},
Month = {WIN},
Abstract = {The current scientific interest in computer thinking is explored for
   constructing a simulated psychodiagnostician. Going beyond simply
   building an expert system, this paper proposes to use a self-learning
   and generalizing computer architecture, State Operator And Result, or
   the SOAR system, to model intelligent clinical behavior. Accordingly, it
