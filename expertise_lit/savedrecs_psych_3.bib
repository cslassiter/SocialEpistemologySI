@article{ WOS:000076689100002,
Author = {Eriksen, EO and Skivenes, M},
Title = {How to make the right decisions in cases of child abuse},
Journal = {TIDSSKRIFT FOR SAMFUNNSFORSKNING},
Year = {1998},
Volume = {39},
Number = {3},
Pages = {352-380},
Abstract = {There is seemingly a low level of trust in the child-care sector in
   Norway. In spite of heavily increased economic resources, a new law, and
   enhanced competence, child care at the street level faces legitimacy
   problems. This may be due to the large amount of discreteness of the
   criteria used for intervening in custody matters and the fact that there
   are few guidelines for decision-making from empirical knowledge and
   professional expertise. Dissent prevails regarding to method, theory,
   and interpretation of scientific data. Precise rules and empirical
   knowledge, nevertheless, do not yield coherent and consistent decisions
   while the notion of the child's best interest is a normative category.
   It says something of the standards that should prevail and is
   consequently something that should be decided on in a dialogue between
   affected parties. Using discourse theory, we examine whether established
   procedures at the local and county level in Norway can ensure fair and
   reasonable decisions. We discover weak points but consider Fylkesnemnda
   (child-care department) an important contribution to establish a fair
   procedure for decision-making in child-care cases.},
