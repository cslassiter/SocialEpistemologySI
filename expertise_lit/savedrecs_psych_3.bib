@article{ WOS:000643817900001,
Author = {Munly, Kelly and Allen, Katherine R. and Roberto, Karen A.},
Title = {Experiences of black women adult foster care providers for aging adults:
   negotiating power and care},
Journal = {JOURNAL OF WOMEN \& AGING},
Year = {2022},
Volume = {34},
Number = {6},
Pages = {692-705},
Month = {NOV 2},
Abstract = {This study explored experiences of 23 Black women owners and operators
   of Adult Foster Care (AFC) homes for midlife and older adults.
   Semi-structured interview data focusing on multiple dimensions of the
   care context were analyzed using grounded theory methods. Women
   leveraged their resources as they invested their expertise, time, and
   relationships to support their residents and embraced the value of
   residents for their contribution to the success of AFC settings. The
   findings underline the critical roles of these AFC providers in
   maintaining safe and home-like care contexts for aging adults, in the
   face of systemic challenges.},
