@article{ WOS:000494429200003,
Author = {LaMeres, Brock J. and Burns, Maxwell S. and Thoman, Dustin B. and Smith,
   Jessi L.},
Title = {The Role of Prosocial Goal Congruity on Student Motivation in Electrical
   Engineering},
