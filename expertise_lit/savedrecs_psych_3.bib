   Bostanovna, Sharipova Asel},
Title = {INFORMATION-PSYCHOLOGICAL SECURITY IN MULTICONFESSIONAL SOCIETY},
Journal = {EUROPEAN JOURNAL OF SCIENCE AND THEOLOGY},
