@article{ WOS:000741856900001,
Author = {Yasseri, Taha and Reher, Jannie},
Title = {Fooled by facts: quantifying anchoring bias through a large-scale
   experiment},
Journal = {JOURNAL OF COMPUTATIONAL SOCIAL SCIENCE},
Year = {2022},
Volume = {5},
Number = {1},
Pages = {1001-1021},
Month = {MAY},
Abstract = {Through a large-scale online field experiment, we provide new empirical
   evidence for the presence of the anchoring bias in people's judgement
   due to irrational reliance on a piece of information that they are
   initially given. The comparison of the anchoring stimuli and respective
   responses across different tasks reveals a positive, yet complex
   relationship between the anchors and the bias in participants'
   predictions of the outcomes of events in the future. Participants in the
   treatment group were equally susceptible to the anchors regardless of
   their level of engagement, previous performance, or gender. Given the
   strong and ubiquitous influence of anchors quantified here, we should
   take great care to closely monitor and regulate the distribution of
   information online to facilitate less biased decision making.},
Publisher = {SPRINGERNATURE},
Address = {CAMPUS, 4 CRINAN ST, LONDON, N1 9XW, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Yasseri, T (Corresponding Author), Univ Coll Dublin, Sch Sociol, Dublin D04 V1W8, Ireland.
   Yasseri, T (Corresponding Author), Univ Coll Dublin, Geary Inst Publ Policy, Dublin D04 N9Y1, Ireland.
