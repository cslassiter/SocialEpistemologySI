@article{ WOS:000381533000017,
Author = {von der Gracht, Heiko A. and Hommel, Ulrich and Prokesch, Tobias and
   Wohlenberg, Holger},
Title = {Testing weighting approaches for forecasting in a Group Wisdom Support
   System environment},
Journal = {JOURNAL OF BUSINESS RESEARCH},
