@article{ WOS:000720409100005,
Author = {Hart, Alexander and Toma, Mariana and Issa, Fadi and Neault, Noelle},
Title = {Developing Cultural Awareness Curricular Competencies for Humanitarian
   Non-Governmental Organization Staff},
Journal = {PREHOSPITAL AND DISASTER MEDICINE},
