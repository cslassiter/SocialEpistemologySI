@article{ WOS:000487014000004,
Author = {Doyle, Casey},
Title = {Aiding self-knowledge},
Journal = {CANADIAN JOURNAL OF PHILOSOPHY},
Year = {2019},
Volume = {49},
Number = {8},
Pages = {1104-1121},
Month = {NOV 17},
Abstract = {Some self-knowledge must be arrived at by the subject herself, rather
   than being transmitted by another's testimony. Yet in many cases the
   subject interacts with an expert in part because she is likely to have
   the relevant knowledge of their mind. This raises a question: what is
   the expert's knowledge like that there are barriers to simply
   transmitting it by testimony? I argue that the expert's knowledge is, in
   some circumstances, proleptic, referring to attitudes the subject would
   hold were she to reflect in certain ways. The expert's knowledge cannot
   be transmitted by testimony because self-knowledge cannot be proleptic.},
