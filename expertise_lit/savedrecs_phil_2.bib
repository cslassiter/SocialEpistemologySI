@article{ WOS:000937592200001,
Author = {Levy, Neil},
Title = {Against Intellectual Autonomy: Social Animals Need Social Virtues},
Journal = {SOCIAL EPISTEMOLOGY},
Abstract = {We are constantly called upon to evaluate the evidential weight of
   testimony, and to balance its deliverances against our own independent
   thinking. `Intellectual autonomy' is the virtue that is supposed to be
   displayed by those who engage in cognition in this domain well. I argue
   that this is at best a misleading label for the virtue, because virtuous
   cognition in this domain consists in thinking with others, and
   intelligently responding to testimony. I argue that the existing label
   supports an excessively individualistic conception of good thinking,
   both within and outside philosophy. I propose replacing `intellectual
   autonomy' with `intellectual interdependence', which properly emphasises
   the depth of our reliance on one another, without suggesting we ought
   ever to be epistemically servile.},
