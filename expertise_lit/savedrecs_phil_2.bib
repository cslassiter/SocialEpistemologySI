@article{ WOS:000884258100001,
Author = {Simoes, Ana},
Title = {In the Shadow of the 1919 Total Solar Eclipse: The Two British
   Expeditions and the Politics of Invisibility},
Journal = {BERICHTE ZUR WISSENSCHAFTSGESCHICHTE},
Year = {2022},
Volume = {45},
