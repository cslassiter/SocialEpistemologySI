@article{ WOS:000461429200004,
Author = {Mizrahi, Moti},
Title = {You Will Respect My Authoritah!? A Reply to Botting},
Journal = {INFORMAL LOGIC},
Year = {2019},
Volume = {39},
Number = {1},
Pages = {106-122},
Abstract = {In Mizrahi (2013a) and (2016a), I argue that arguments from expert
   opinion are weak. To appeal to expert opinion is to take an expert's
   judgment that p is the case as (defeasible) evidence for p. Appeals to
   expert opinion are weak because the fact that an expert judges that p
   doesn't make it significantly more likely that p is true. Unlike other
   critics, Botting (2018) says he wants to take issue with the premise
   that reliability is a necessary condition for strong appeals to expert
   opinion. I respond to Botting's objections and argue that they miss
   their intended target.},
