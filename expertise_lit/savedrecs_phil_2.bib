@article{ WOS:000696797400003,
Author = {Blado, Joseph E.},
Title = {The Extended-Expert-As-Teacher (EEAT) Model A Defense of De Cruz},
Journal = {GRAZER PHILOSOPHISCHE STUDIEN-INTERNATIONAL JOURNAL FOR ANALYTIC
   PHILOSOPHY},
