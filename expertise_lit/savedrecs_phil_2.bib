@article{ WOS:000451998300004,
Author = {Aidnik, Martin and Rikmann, Erle},
Title = {Domesticating the future?: Citizen's income discussion in Estonia},
Journal = {JOURNAL OF BALTIC STUDIES},
Year = {2018},
Volume = {49},
Number = {4},
Pages = {487-508},
Abstract = {Citizen's income is an idea that is currently gaining ground in many
   parts of the world. The prospect of disappearing jobs and social
   exclusion that the globalized world faces has given the idea of
   guaranteed income a new urgency. This article is a pilot sociological
   study of the citizen's income discussion in post-socialist Estonia. The
   article is based on qualitative research: interviews with experts from
   different fields and an analysis of print media. Four systematic
   approaches to citizen's income are differentiated in the paper. We
   analyze the views on the idea in order to explore their significance for
   the Estonian economic status quo and welfare state.},
