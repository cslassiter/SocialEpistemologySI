@article{ WOS:000934609200001,
Author = {Sa, Rentuya and Alcock, Lara and Inglis, Matthew and Tanswell, Fenner
   Stanley},
Title = {Do Mathematicians Agree about Mathematical Beauty?},
Journal = {REVIEW OF PHILOSOPHY AND PSYCHOLOGY},
Abstract = {Mathematicians often conduct aesthetic judgements to evaluate
   mathematical objects such as equations or proofs. But is there a
   consensus about which mathematical objects are beautiful? We used a
   comparative judgement technique to measure aesthetic intuitions among
   British mathematicians, Chinese mathematicians, and British mathematics
   undergraduates, with the aim of assessing whether judgements of
   mathematical beauty are influenced by cultural differences or levels of
   expertise. We found aesthetic agreement both within and across these
   demographic groups. We conclude that judgements of mathematical beauty
   are not strongly influenced by cultural difference, levels of expertise,
   and types of mathematical objects. Our findings contrast with recent
   studies that found mathematicians often disagree with each other about
   mathematical beauty.},
