@article{ WOS:000524936800008,
Author = {Ichikawa, Jonathan Jenkins},
Title = {FAITH AND EPISTEMOLOGY},
Journal = {EPISTEME-A JOURNAL OF INDIVIDUAL AND SOCIAL EPISTEMOLOGY},
