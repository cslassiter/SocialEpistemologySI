@article{ WOS:000592962000005,
Author = {Goldenbaum, Ursula},
Title = {Did Moses Mendelssohn Lack Historical Thinking?},
Journal = {DEUTSCHE ZEITSCHRIFT FUR PHILOSOPHIE},
Year = {2020},
Volume = {68},
Number = {4},
Pages = {564-589},
Month = {AUG},
Abstract = {There is widespread agreement in scholarship that Moses Mendelssohn
   lacked historical thinking, an opinion accepted even among Mendelssohn
   experts. This misjudgment is based on a remark in his Jerusalem against
   Lessing's Education of Humankind and surely ignores Mendelssohn's
   historical work. I will question the misjudgment by a detour: first, I
   will ask for whom Lessing wrote his Education of Humankind. Then I will
   turn to the usually celebrated origin of historical thinking in Semler
   and Herder and question the historicity of their views. It is only in
   the 3rd section that I will focus directly on Mendelssohn's historical
   work and his truly historical understanding of religion, in agreement
   with Lessing.},
