@article{ WOS:000384462200015,
Author = {Hornbeck, Stephanie E. and Moffett, Dana L.},
Title = {Altered surfaces, taking the long view: Applications of ethnographic
   conservation practices to the conservation of contemporary art},
Journal = {STUDIES IN CONSERVATION},
Year = {2016},
Volume = {61},
Number = {2, SI},
Pages = {84-90},
Abstract = {While working together on tradition-based and contemporary works at the
   Smithsonian's National Museum of African Art (NMAfA) and in private
   practice, the authors found that their expertise in ethnographic
   conservation can be applied to the conservation of contemporary works.
   The treatment approach to ethnographic works has evolved over decades,
   shifting from alteration of original surfaces and heavy restoration to
   an emphasis on preserving the original integrity of the artwork through
   minimal intervention. This paper describes the evolution of ethnographic
   object conservation regarding surface alteration, while specifically
   focusing on African objects, and discusses applications of this approach
   to the conservation of contemporary art. The historical change in
   treatment attitudes is presented using examples from NMAfA's collections
   to underscore why the profession developed a minimal intervention
   approach and to argue for the relevance of this approach to related
   contemporary media.},
