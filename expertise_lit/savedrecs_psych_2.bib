@article{ WOS:000285311900006,
Author = {Liley, James and Rakow, Tim},
Title = {Probability Estimation in Poker: A Qualified Success for Unaided
   Judgment},
Journal = {JOURNAL OF BEHAVIORAL DECISION MAKING},
