@article{ WOS:000385609000020,
Author = {Gore, Julie and Conway, Gareth E.},
Title = {Modeling and Aiding Intuition in Organizational Decision Making: A Call
   for Bridging Academia and Practice},
Journal = {JOURNAL OF APPLIED RESEARCH IN MEMORY AND COGNITION},
Year = {2016},
