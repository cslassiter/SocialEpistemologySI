@article{ WOS:A1997WY16300001,
Author = {Marsh, KL and HartORourke, DM and Julka, DL},
Title = {The persuasive effects of verbal and nonverbal information in a context
   of value relevance},
Journal = {PERSONALITY AND SOCIAL PSYCHOLOGY BULLETIN},
Year = {1997},
Volume = {23},
Number = {6},
Pages = {563-579},
Month = {JUN},
Abstract = {These studies investigated how the verbal and nonverbal content of a
   message affected value-based attitudes. Participants viewed a speaker
   who advocated a policy change at their own university
   (high-personal-relevance condition) or another university (low
   personal-relevance condition) on an issue relevant to participants'
   values. Verbal content of the message (strong or weak arguments) and
   nonverbal behavior of the speaker (negative or natural) were varied. A
   negative delivery led to impressions of the speaker as less likable,
   assured, and expert, but it had no effect on attitudes. Increasing
   personal relevance led to more negative attitudes, but argument strength
   affected the attitudes and thoughts of both low- and
   high-personal-relevance participants. Results suggested that for a
   value-relevant topic, lowering personal relevance did not reduce
   extensive processing of verbal content.},
