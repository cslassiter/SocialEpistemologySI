@article{ WOS:000232053500002,
Author = {Posavac, SS and Kardes, FR and Sanbonmatsu, DM and Fitzsimons, GJ},
Title = {Blissful insularity: When brands are judged in isolation from
   competitors},
Journal = {MARKETING LETTERS},
Year = {2005},
Volume = {16},
Number = {2},
Pages = {87-97},
Month = {APR},
Abstract = {The need to evaluate a single brand in isolation, without explicit
   comparisons to competitors, is ubiquitous in consumer judgment because
   competitors are often not readily apparent. Although consumers routinely
   make such judgments, in this paper we show that when a brand is judged
   in isolation, the judgment is often overly favorable. Moreover, we
   explore when and why this tendency occurs. Data from 259 participants
   across 3 experiments that considered very different product categories,
   and that were conducted with student and adult consumer samples,
   converge to show that; (1) isolated brand evaluations on average are
   characterized by a favorableness bias, (2) this bias results from
   consumers' selective processing of information about the focal brand,
   (3) favorableness bias is attenuated if context leads consumers to
   consider alternatives to a salient focal brand, and (4) the isolated
   brand judgments of experts are much better calibrated than are those of
   novices.},
