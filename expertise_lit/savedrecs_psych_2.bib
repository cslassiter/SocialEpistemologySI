@article{ WOS:A1994PY03600019,
Author = {VONHIPPEL, W and HAWKINS, C and NARAYAN, S},
Title = {PERSONALITY AND PERCEPTUAL EXPERTISE - INDIVIDUAL-DIFFERENCES IN
   PERCEPTUAL IDENTIFICATION},
Journal = {PSYCHOLOGICAL SCIENCE},
Year = {1994},
Volume = {5},
Number = {6},
Pages = {401-406},
Month = {NOV},
Abstract = {We hypothesized that people become expert at perceiving information that
   is related to concepts they think about a great deal, because of their
   extensive perceptual experience with this material.  To test this idea,
   we manipulated the capitalization of a series of briefly exposed words. 
