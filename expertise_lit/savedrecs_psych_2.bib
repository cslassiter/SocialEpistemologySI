@article{ WOS:000386914100005,
Author = {Magierowski, Mateusz},
Title = {(A)symmetry of (Non-)memory: The Missed Opportunity to Work Through the
   Traumatic Memory of the Polish-Ukrainian Ethnic Conflict in Pawokoma},
Journal = {EAST EUROPEAN POLITICS AND SOCIETIES},
