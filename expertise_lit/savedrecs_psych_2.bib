@article{ WOS:000415721300002,
Author = {Lohre, Erik and Teigen, Karl Halvor},
Title = {Probabilities Associated with Precise and Vague Forecasts},
Journal = {JOURNAL OF BEHAVIORAL DECISION MAKING},
