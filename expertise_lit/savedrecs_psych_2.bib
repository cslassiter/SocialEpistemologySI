@article{ WOS:A1991GW72000003,
Author = {PEREZ, JA and MOSCOVOCI, S and MUGNY, G},
Title = {RESISTANCE TO AN EXPERT OR MINORITY SOURCE AND ATTITUDE-CHANGE},
Journal = {SCHWEIZERISCHE ZEITSCHRIFT FUR PSYCHOLOGIE-REVUE SUISSE DE PSYCHOLOGIE},
Year = {1991},
Volume = {50},
Number = {4},
Pages = {260-267},
Abstract = {We studied the intention to stop smoking of 180 smokers before and after
   having exposed them to an anti-tobacco message. According to a 2 x 2 x 2
   design, the source of the message was presented as expert (professors of
   political economy) or minority (militants of a minority group); subjects
   had to criticize either the source or the content of the message; they
   were informed or not that a well founded criticism has to contain at
   least 8 arguments. Changes in the intention to stop smoking, predicted
   from studies on resistances to influence such as denial and
   psychologization, revealed a significant interaction between source
   identity and the nature of the critical activity. The high-credibility
   source has more influence when subjects are focused on the source,
   whereas the minority is more so when subjects are focused on the content
   of the message. These effects suggest that the widely spread belief,
   according to which recourse to experts is necessary to influence public
   opinion, has to be corrected and envisage the possible positive effects
   of other forms of influence - those of minorities on particular.},
