Title = {``Soft{''} Versus ``Hard{''} Psychological Science: Biased Evaluations
   of Scientific Evidence That Threatens or Supports a Strongly Held
   Political Identity},
Journal = {BASIC AND APPLIED SOCIAL PSYCHOLOGY},
