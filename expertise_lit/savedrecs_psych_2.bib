@article{ WOS:000328541400007,
Author = {Tzioti, Stefanie C. and Wierenga, Berend and van Osselaer, Stijn M. J.},
Title = {The Effect of Intuitive Advice Justification on Advice Taking},
Journal = {JOURNAL OF BEHAVIORAL DECISION MAKING},
