@article{ WOS:A1995RB73400010,
Author = {SOLOMON, S and GREENBERG, J and PSYZCZYNSKI, T and PRYZBYLINSKI, J},
Title = {THE EFFECTS OF MORTALITY SALIENCE ON PERSONALLY-RELEVANT PERSUASIVE
   APPEALS},
Journal = {SOCIAL BEHAVIOR AND PERSONALITY},
Year = {1995},
Volume = {23},
Number = {2},
Pages = {177-190},
Abstract = {The effects of mortality salience on personally-relevant persuasive
   appeals were investigated by exposing subjects to strong or weak
   arguments about a personally-relevant issue by an expert or nonexpert
   source following a mortality salience or control induction, and then
   measuring subjects' attitudes about that issue. A significant mortality
   salience X argument source X strength of arguments interaction was
   found. In control conditions, subjects were more persuaded by strong
   than weak arguments when the source of the persuasive appeal was a
