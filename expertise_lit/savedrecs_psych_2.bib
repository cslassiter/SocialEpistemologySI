@article{ WOS:000265737700003,
Author = {Gula, Bartosz and Koeppen, Joern},
Title = {Influence of length and perfection of a ``hot hand{''} sequence on
   allocation decisions in volleyball},
Journal = {ZEITSCHRIFT FUR SPORTPSYCHOLOGIE},
Year = {2009},
Volume = {16},
Number = {2},
Pages = {65-70},
Abstract = {In research on the ``hot hand{''} belief the question of the extent to
   which the belief influences allocation decisions is largely unsettled.
   The present study examined whether two essential aspects of an offensive
   sequence that are relevant to the belief factually influence allocation
   behavior: the length of sequences of hits and their perfection. Study
   participants were presented with video clips of Successful and
   unsuccessful spikes in volleyball and had to make allocation decisions
   to two players on each try. The results show that the structure and
   length of hot hand sequences differentially influence allocation
   decisions of participants with different levels of expertise.},
