@article{ WOS:000183116100007,
Author = {Thomson, ME and Onkal-Atay, D and Pollock, AC and Macaulay, A},
Title = {The influence of trend strength on directional probabilistic currency
   predictions},
Journal = {INTERNATIONAL JOURNAL OF FORECASTING},
Year = {2003},
Volume = {19},
Number = {2},
Pages = {241-256},
Month = {APR-JUN},
Abstract = {An experiment is reported which compares the judgmental forecasting
   performance of experts and novices using simulated currency series with
   differing trend strengths. Analyses of directional probability forecasts
   reveal: (1) significant effects of trend strength on all aspects of
   predictive performance being studied, with evidence for the hard-easy
   effect where overconfidence is exhibited on weak (i.e., more difficult)
   trends. while underconfidence is shown on strong (i.e., less difficult)
   trends; (2) lower performance of experts on relative accuracy and
   profitability measures. reflecting experts' resistance to strong trends;
   (3) better overall performance on negative trends; and (4) superior
   performance of composite forecasts. Possible explanations are offered
   for these results and future research directions are outlined. (C) 2001
   International Institute of Forecasters. Published by Elsevier Science
   B.V. All rights reserved.},
