@article{ WOS:000256087900007,
Author = {Leung, Patrick W. and Trotman, Ken T.},
Title = {Effect of different types of feedback on the level of auditors'
   configural information processing},
Journal = {ACCOUNTING AND FINANCE},
Year = {2008},
Volume = {48},
Number = {2},
Pages = {301-318},
Month = {JUN},
Abstract = {Previous audit judgement research has emphasized the importance of
   auditors processing information configurally (i.e. jointly considering
   the impact of different cues). This paper examines the impact of four
   different forms of feedback (outcome, task properties, cognitive and
   combined) on increasing the extent of configural information processing
   by auditors. We find that task properties, cognitive and combined (task
   properties plus cognitive) feedback all lead to increased configural
   information processing. There is no impact of outcome feedback. We also
   find that the extent of configural processing is positively associated
   with our measure of judgement performance. We discuss the implications
   of the results for audit firms.},
