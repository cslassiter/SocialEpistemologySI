@article{ WOS:A1994QF65500001,
Author = {HEIT, E and PRICE, PC and BOWER, GH},
Title = {A MODEL FOR PREDICTING THE OUTCOMES OF BASKETBALL GAMES},
Journal = {APPLIED COGNITIVE PSYCHOLOGY},
Year = {1994},
Volume = {8},
Number = {7},
